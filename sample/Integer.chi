
native struct Integer {
	lambda add : Integer -> Integer;
	lambda + = add;
	lambda sub : Integer -> Integer;
	lambda - = sub;
	lambda muti : Integer -> Integer;
	lambda * = muti;
	lambda div : Integer -> Integer;
	lambda / = div;
	lambda mod : Integer -> Integer;
	lambda % = mod;

	lambda < : Integer -> Boolean = i -> self - i < 0
	lambda <= : Integer -> Boolean = i -> self - i <= 0;
	lambda > : Integer -> Boolean = i -> i < self;
	lambda >= : Integer -> Boolean = i -> i <= self;
}